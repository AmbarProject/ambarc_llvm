fib_result: int;

func main() -> int {
    n: int = 1000;

    a: int = 0;
    b: int = 1;
    temp: int;

    if (n == 0) {
        fib_result = a;
    } else {
        for (i:int = 2; i <= n; i = i + 1) {
            temp = a + b;
            a = b;
            b = temp;
        }
        fib_result = b;
    }

    return 0;
}

